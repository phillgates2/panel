{
  "dashboard": {
    "id": null,
    "title": "Panel System Monitoring",
    "tags": ["panel", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System CPU Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [{
          "expr": "panel_cpu_usage_percent",
          "legendFormat": "CPU Usage %"
        }],
        "yAxes": [
          {"format": "percent", "min": 0, "max": 100},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "System Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [{
          "expr": "panel_memory_usage_percent",
          "legendFormat": "Memory Usage %"
        }],
        "yAxes": [
          {"format": "percent", "min": 0, "max": 100},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Disk Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [{
          "expr": "panel_disk_usage_percent",
          "legendFormat": "Disk Usage %"
        }],
        "yAxes": [
          {"format": "percent", "min": 0, "max": 100},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Database Connections",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "panel_db_pool_size",
            "legendFormat": "Pool Size"
          },
          {
            "expr": "panel_db_connections_checked_out",
            "legendFormat": "Checked Out"
          }
        ]
      },
      {
        "id": 5,
        "title": "Redis Metrics",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "panel_redis_connected_clients",
            "legendFormat": "Connected Clients"
          },
          {
            "expr": "panel_redis_used_memory_mb",
            "legendFormat": "Used Memory (MB)"
          }
        ]
      },
      {
        "id": 6,
        "title": "Application Users",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "panel_app_total_users",
            "legendFormat": "Total Users"
          },
          {
            "expr": "panel_app_active_users",
            "legendFormat": "Active Users"
          }
        ]
      },
      {
        "id": 7,
        "title": "Network I/O",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "panel_network_bytes_sent_mb",
            "legendFormat": "Bytes Sent (MB)"
          },
          {
            "expr": "panel_network_bytes_recv_mb",
            "legendFormat": "Bytes Received (MB)"
          }
        ]
      },
      {
        "id": 8,
        "title": "Application Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "panel_app_request_count",
            "legendFormat": "Request Count"
          },
          {
            "expr": "panel_app_error_count",
            "legendFormat": "Error Count"
          }
        ]
      },
      {
        "id": 9,
        "title": "System Uptime",
        "type": "singlestat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 32},
        "targets": [{
          "expr": "panel_system_uptime_seconds / 3600",
          "legendFormat": "Uptime (hours)"
        }],
        "format": "duration",
        "colorBackground": true,
        "thresholds": "24,168"
      },
      {
        "id": 10,
        "title": "Total Servers",
        "type": "singlestat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 32},
        "targets": [{
          "expr": "panel_app_total_servers",
          "legendFormat": "Total Servers"
        }],
        "format": "short"
      },
      {
        "id": 11,
        "title": "Application Uptime",
        "type": "singlestat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 32},
        "targets": [{
          "expr": "panel_app_uptime_seconds / 3600",
          "legendFormat": "App Uptime (hours)"
        }],
        "format": "duration",
        "colorBackground": true,
        "thresholds": "1,24"
      },
      {
        "id": 12,
        "title": "Redis Uptime",
        "type": "singlestat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 32},
        "targets": [{
          "expr": "panel_redis_uptime_days",
          "legendFormat": "Redis Uptime (days)"
        }],
        "format": "short",
        "colorBackground": true,
        "thresholds": "1,7"
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}