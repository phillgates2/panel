{
  "dashboard": {
    "title": "Panel Application Dashboard",
    "tags": ["panel", "flask", "game-server"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "rate(flask_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "unit": "reqps",
            "label": "Requests/sec"
          },
          {
            "unit": "short"
          }
        ]
      },
      {
        "id": 2,
        "title": "Response Time (95th percentile)",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(flask_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(flask_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "unit": "s",
            "label": "Response Time"
          },
          {
            "unit": "short"
          }
        ]
      },
      {
        "id": 3,
        "title": "Active Users",
        "type": "singlestat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "panel_active_users",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 1000
                }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Registered Users",
        "type": "singlestat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 8
        },
        "targets": [
          {
            "expr": "panel_registered_users_total",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "title": "Active Servers",
        "type": "singlestat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "panel_active_servers",
            "refId": "A"
          }
        ]
      },
      {
        "id": 6,
        "title": "Total Players",
        "type": "singlestat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 8
        },
        "targets": [
          {
            "expr": "sum(panel_server_players)",
            "refId": "A"
          }
        ]
      },
      {
        "id": 7,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        },
        "targets": [
          {
            "expr": "rate(panel_errors_total[5m])",
            "legendFormat": "{{error_type}} errors",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "unit": "errors/sec",
            "label": "Errors/sec"
          },
          {
            "unit": "short"
          }
        ]
      },
      {
        "id": 8,
        "title": "Database Connections",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        },
        "targets": [
          {
            "expr": "panel_db_connections_active",
            "legendFormat": "Active connections",
            "refId": "A"
          }
        ]
      },
      {
        "id": 9,
        "title": "Cache Performance",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 20
        },
        "targets": [
          {
            "expr": "rate(panel_cache_hits_total[5m])",
            "legendFormat": "Cache hits",
            "refId": "A"
          },
          {
            "expr": "rate(panel_cache_misses_total[5m])",
            "legendFormat": "Cache misses",
            "refId": "B"
          }
        ]
      },
      {
        "id": 10,
        "title": "Business Metrics",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 20
        },
        "targets": [
          {
            "expr": "rate(panel_donations_total[1h])",
            "legendFormat": "Donations/hour",
            "refId": "A"
          },
          {
            "expr": "rate(panel_forum_posts_total[1h])",
            "legendFormat": "Forum posts/hour",
            "refId": "B"
          }
        ]
      }
    ]
  }
}